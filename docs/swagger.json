                }
            }
        },
        "models.DelParentRelationshipRes": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "object",
                    "properties": {
                        "string": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "models.DelPerson": {
